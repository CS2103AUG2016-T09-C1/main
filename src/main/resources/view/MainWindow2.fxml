<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="background" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.inbx0.ui.MainWindow">
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
   <children>
      <MenuBar stylesheets="@DarkTheme.css" VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane fx:id="commandBoxPlaceholder" prefHeight="35.0" prefWidth="1302.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </AnchorPane>
       <AnchorPane fx:id="resultDisplayPlaceholder" prefHeight="114.0" prefWidth="1302.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
       </AnchorPane>
  <AnchorPane fx:id="mainDisplayPlaceholder" prefHeight="758.0" prefWidth="1302.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </AnchorPane>
      <AnchorPane fx:id="statusbarPlaceholder" prefHeight="36.0" prefWidth="1443.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin></AnchorPane>
   </children>
</VBox>
